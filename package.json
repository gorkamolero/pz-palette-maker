{
  "name": "pz-palette-maker",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "pui": "npm config set '@bit:registry' https://node.bit.dev && bit init && bit login",
    "wc": "vue-cli-service build --target wc --name pz-palette-maker ./src/main.js"
  },
  "dependencies": {
    "@bit/pickzen.pui.core.pz-boiler": "^1.0.0",
    "@bit/pickzen.pui.core.typer": "^0.0.1",
    "chroma-js": "^2.0.4",
    "core-js": "^2.6.5",
    "material-icons": "^0.3.1",
    "node-vibrant": "^3.1.3",
    "uppload-vue": "^1.1.2",
    "vue": "^2.6.10",
    "vue-color": "^2.7.0",
    "vue-cute-modal": "^1.1.0",
    "vue-modal-dialogs": "^3.0.0",
    "vue-observe-visibility": "^0.4.4",
    "vue-router": "^3.0.3",
    "vue-slicksort": "^1.1.3",
    "vue-swatches": "^1.0.3",
    "vue2-animate": "^2.1.0",
    "vuesax": "^3.8.74",
    "vuex": "^3.0.1"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.8.0",
    "@vue/cli-service": "^3.8.0",
    "normalize-scss": "^7.0.1",
    "sass": "^1.21.0",
    "sass-loader": "^7.1.0",
    "vue-cli-plugin-pz-boiler": "^0.1.3",
    "vue-template-compiler": "^2.6.10"
  },
  "bit": {
    "env": {
      "compiler": "bit.envs/bundlers/vue@2.5.0"
    },
    "componentsDefaultDirectory": "src/components/{name}",
    "packageManager": "yarn",
    "overrides": {
      "pickzen.pui/core/styles": {
        "env": {
          "compiler": "bit.envs/bundlers/vue@2.5.0"
        }
      }
    }
  }
}
